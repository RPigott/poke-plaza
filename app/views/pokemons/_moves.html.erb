<div class="moves-container">
<% [pokemon.move1, pokemon.move2].each do |move| %>
	<%= render "moves/move", egg: pokemon.species.egg_moves.include?(move), move: move %>
<% end %>
</div>
<div class="moves-container">
<% [pokemon.move3, pokemon.move4].each do |move| %>
	<%= render "moves/move", egg: pokemon.species.egg_moves.include?(move), move: move %>
<% end %>
</div>